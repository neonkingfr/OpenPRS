# $Id$

prefix=		@prefix@
exec_prefix=	@exec_prefix@
abs_srcdir=	@abs_srcdir@

# Which compiler to use (gcc is prefered).
CC = @CC@
CFLAGS = @CFLAGS@

YACC = @BISON@
YFLAGS = -d

LEX = @FLEX@
LEXFLAGS =
LEXLIB = -L$(subst bin,lib,$(patsubst %flex,%,@FLEX@)) -lfl

MV = mv

LN_S = @LN_S@

PERL = @PERL@

MKDEP 		= @MKDEP@

INSTALL = @abs_top_srcdir@/util/install.sh
INSTALL-IF-CHANGE = @abs_top_srcdir@/util/install-if-change.sh


LD = ld				#Strange... autoconf does not care
RANLIB = @RANLIB@
PURIFY =
OPTIMIZE = -O
DEBUG_FLAG = -g
STATIC =
ANSI = 
WARNINGS = -Wall

MEMLIB_DEB = -DDO_CHECK=0


# Where are the X11 and Motif libs on this machine.

X_LIB = @MOTIF_LIBS@ @X_LIBS@ @X_EXTRA_LIBS@ -lXt -lX11

# and the X include.

X_INCLUDE = @MOTIF_CFLAGS@ @X_CFLAGS@

EXTRA_LIB = @LIBS@

EXTRA_LIB += @READLINE_LIBS@

READLINE_INCLUDE = @READLINE_INCLUDES@

EXEC-PREFIX = @exec_prefix@
BINDIR = @bindir@
LIBDIR = @libdir@
INCLUDEDIR = @includedir@
DATADIR = @prefix@/data
DOCDIR = @prefix@/doc
DESTDIR = @prefix@


PRPC_LANG = en

USE_MULTI_THREAD = 1

ifdef USE_MULTI_THREAD
MT_LDFLAGS =
MT_CPPFLAGS =
endif

